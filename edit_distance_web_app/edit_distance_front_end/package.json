{
  "name": "edit_distance_front_end",
  "homepage": ".",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "bootstrap": "^4.5.2",
    "copyfiles": "^2.3.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-cookies": "^0.1.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "renamer": "^2.0.1",
    "styled-components": "^5.2.1",
    "whatwg-fetch": "^3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "collect": "react-scripts build && yarn run build-rename && yarn run copy-buildfiles && yarn run collectstatic",
    "build-rename": "yarn run build-rename-js && yarn run build-rename-js1 && yarn run build-rename-js2 && yarn run build-rename-css && yarn run build-rename-logo",
    "build-rename-logo": "renamer --find '/([^\\.]*)\\.[^\\.]+\\.(\\w+)/g' --replace '$1.$2' build/static/media/*",
    "build-rename-js": "renamer --find '/main\\.[^\\.]+\\.chunk.js/g' --replace 'edit_distance_front_end.ui.js' build/static/js/*.js",
    "build-rename-js1": "renamer --find '/runtime-main\\.[^\\.]+\\.js/g' --replace 'runtime-main.edit_distance_front_end.ui.js' build/static/js/*.js",
    "build-rename-js2": "renamer --find '/2\\.[^\\.]+\\.chunk.js/g' --replace '2.edit_distance_front_end.ui.js' build/static/js/*.js",
    "build-rename-css": "renamer --find '/main\\.[^\\.]+\\.chunk.css/g' --replace 'edit_distance_front_end.ui.css' build/static/css/*.css",
    "copy-buildfiles": "yarn run copy-build-js && yarn run copy-build-css && yarn run copy-build-logo",
    "copy-build-logo": "copyfiles -f 'build/static/media/*' '../editdistance/staticfiles/media'",
    "copy-build-js": "copyfiles -f 'build/static/js/*.js' '../editdistance/staticfiles/js'",
    "copy-build-css": "copyfiles -f 'build/static/css/*.css' '../editdistance/staticfiles/css'",
    "collectstatic": "python ../manage.py collectstatic --no-input"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
